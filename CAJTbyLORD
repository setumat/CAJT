local Tab = Window:CreateTab("Teleport", "arrow-big-right-dash") -- Title, Image

local Section = Tab:CreateSection("Select World")

-- ===========================
-- TELEPORT VARIABLE
-- ===========================

local teleportID = 1      -- default id (sesuai world default)

-- map pilihan teleport -> id
local teleportToID = {
   ["World 1"] = 1,
   ["World 2"] = 2,
   ["World 3"] = 3,
   ["World 4"] = 4,
   ["World 5"] = 5,
   ["World 6"] = 6,
   ["World 7"] = 7,
   ["World 8"] = 8,
   ["World 9"] = 9,
   ["World 10"] = 10,
   ["World 11"] = 11,
   ["World 12"] = 12,
   ["World 13"] = 13,
   ["World 14"] = 14,
   ["World 15"] = 15,
   ["World 16"] = 16,
   ["World 17"] = 17,
   ["World 18"] = 18,
   ["World 19"] = 19,
   ["World 20"] = 20,
   ["World 21"] = 21,
   ["World 22"] = 22
}

-- ===========================
-- TELEPORT UI
-- ===========================

-- teleport dropdown
local Dropdown = Tab:CreateDropdown({
   Name = "World Teleport",
   Options = {
      "World 1",
      "World 2",
      "World 3",
      "World 4",
      "World 5",
      "World 6",
      "World 7",
      "World 8",
      "World 9",
      "World 10",
      "World 11",
      "World 12",
      "World 13",
      "World 14,
      "World 15",
      "World 16",
      "World 17",
      "World 18",
      "World 19",
      "World 20",
      "World 21",
      "World 22"
   },
   CurrentOption = {"World 1"},
   MultipleOptions = false,
   Flag = "WorldTeleportDropdown",
   Callback = function(option)
       teleportID = teleportToID[option[1]] -- update ID sesuai pilihan
       print("Selected World ID : ", teleportID)
   end,
})

-- teleport button
local Button = Tab:CreateButton({
   Name = "Teleport",
   Callback = function()
       local args = {
           "TeleportToTargetWorld",
           teleportID
       }
       game:GetService("ReplicatedStorage"):WaitForChild("Msg"):WaitForChild("RemoteEvent"):FireServer(unpack(args))
       print("Teleporting to World ID  :", teleportID)
   end,
})
